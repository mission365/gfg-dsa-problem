class Solution {
  public:
    string reverseString(string& s) {
        // Your code goes here
        int n = s.size();
        vector<char> ch;
        for(auto it: s){
            ch.push_back(it);
        }
        string ss;
        reverse(ch.begin(),ch.end());
        for(auto it: ch){
            ss+=it;
        }
        return ss;
    }
};
